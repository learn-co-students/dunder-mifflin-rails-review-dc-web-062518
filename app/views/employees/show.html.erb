<h1>Employee</h1>
<h2><%= @employee.first_name %> <%= @employee.last_name %></h2><br>
<%= image_tag(@employee.img_url) %><br>
Alias: <%= @employee.alias %><br>
Title: <%= @employee.title %><br>
Office: <%= @employee.office %><br>
Dog: <%= @employee.dog.name %><br>
<%= link_to "Edit", edit_employee_path(@employee) %>
<%= form_for @employee, method: :delete do |f| %>
<% f.submit "DELETE" %>
<% end %>
